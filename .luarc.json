{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "Lua": {
        "runtime": {
            "version": "LuaJIT",
            "path": [
                "?.lua",
                "?/init.lua"
            ]
        },
        "diagnostics": {
            "globals": [
                "vim"
            ]
        },
        "workspace": {
            "library": [
                "~/.local/share/nvim/lazy/plenary.nvim/lua"
            ],
            "checkThirdParty": false
        },
        "telemetry": {
            "enable": false
        }
    }
}